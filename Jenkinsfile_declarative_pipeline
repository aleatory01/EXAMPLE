pipeline {
  agent any
  stages
  {
    stage ("Welcome")
    {
      steps
        echo "Welcome to Jenkins declaratives"
        sh 'git --version'
      }
    stage ("Git version")
    {
      steps ("Git version")
      {
        sh 'chmod +x hello.py'
        sh '/usr/bin/python3 hello.py'
      }
    }  
  }
}