pipeline:
  agent:
    any:
  stages:
    - stage: "Log versions"
      steps:
        - sh "git --version"
    - stage: "Jenkins YAML Builder"
      steps:
        - sh "git --version"
    - stage: "Final Jenkins YAML Pipeline Stage"
      steps:
        - sh "echo 'Jenkins YAML Pipeline Complete'"

    - stage: "TEST"
      steps:
        - sh "ls -al"
        - sh "pwd"
        - sh "hostname"
        - sh "chmod u+x ./hello.py"
        - sh "./hello.py"

